GEMINI_API_KEY=PLACEHOLDER_API_KEY

# GoHighLevel Configuration
GHL_API_KEY=your_api_key_here
GHL_LOCATION_ID=your_location_id_here
GHL_API_BASE_URL=https://rest.gohighlevel.com/v1
GHL_API_TIMEOUT=10000

# Rate Limiting
RATE_LIMIT_MAX_REQUESTS_PER_IP=5
RATE_LIMIT_WINDOW_MINUTES=15
RATE_LIMIT_MAX_REQUESTS_PER_EMAIL=10
RATE_LIMIT_EMAIL_WINDOW_HOURS=1

# Security
REQUEST_SIGNING_SECRET=generate_random_32_char_string_here
ALLOWED_ORIGINS=https://yourdomain.com,https://www.yourdomain.com

# Feature Flags
ENABLE_PROFANITY_FILTER=false
NODE_ENV=production
